  "homepage": "https://github.com/linkeddata/solid-app-set",
  "dependencies": {
    "better-simple-slideshow": "linkeddata/better-simple-slideshow#gh-pages",
    "mime-types": "^2.1.13",
   "solid-ui": "file://../../linkeddata/solid-ui"
   },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "standard": "^10.0.2",
